{
    "name": "Phishing Detector",
    "description": "Prevent phishing attacks by selectively injecting a warning or safe icon on login windows.",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts":[
      {
        "matches": ["<all_urls>"],
        "js": ["injectIconToPage.js"],
        "css": ["injectIconToPage.css"]
      }
    ],
    "permissions": ["activeTab", "background", "scripting", "storage", "tabs", "unlimitedStorage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/logo_16.png",
          "32": "/images/logo_32.png",
          "48": "/images/logo_48.png",
          "128": "/images/logo_128.png"
        }
      },
    "icons": {
        "16": "/images/logo_16.png",
        "32": "/images/logo_32.png",
        "48": "/images/logo_48.png",
        "128": "/images/logo_128.png"
    }
  }