{
    "name": "Phishing Detector",
    "description": "Building an intuitive and efficient Phishing warning Tool",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts":[
      {
        "matches": ["<all_urls>"],
        "js": ["injectIconToPage.js", "require.js"],
        "css": ["injectIconToPage.css"]
      }
    ],
    "permissions": ["background", "tabs", "activeTab", "storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/logo_16.png",
          "32": "/images/logo_32.png",
          "48": "/images/logo_48.png",
          "128": "/images/logo_128.png"
        }
      },
    "icons": {
        "16": "/images/logo_16.png",
        "32": "/images/logo_32.png",
        "48": "/images/logo_48.png",
        "128": "/images/logo_128.png"
    },
    "options_page": "infopage.html"
  }